  "criticReviews": [
      {
          "iconImage": {
              "url": "https://images.fandango.com/cms/assets/7d9d62c0-9be3-11eb-bd50-d166e33e886a--rotten.png"
          },
          "criticName": "William Jones",
          "publicationName": "CBR",
          "body": "Thor: Love and Thunder's editing routinely works against Taika Waititi's film, often undoing the emotional work the film is attempting to deliver.",
          "url": "https://www.cbr.com/thor-love-thunder-poor-editing/"
      },
      {
          "iconImage": {
              "url": "https://images.fandango.com/cms/assets/7d9d62c0-9be3-11eb-bd50-d166e33e886a--rotten.png"
          },
          "criticName": "Joseph Garcia",
          "publicationName": "Dynamic Duel Podcast",
          "body": "Taika Waititi and Chris Hemsworth leaned heavily into the more goofy aspects of Thor&#58; Ragnarok&#46; Thor&#58; Love and Thunder piles on the jokes&#44; bathos and stupidity until the film feels more like a parody of the MCU rather an addition to it&#46;",
          "url": "https://www.dynamicduel.com/thor-love-and-thunder-review/"
      },
      {
          "iconImage": {
              "url": "https://images.fandango.com/cms/assets/7d9d62c0-9be3-11eb-bd50-d166e33e886a--rotten.png"
          },
          "criticName": "Eric Marchen",
          "publicationName": "Rogers TV",
          "body": "I&#8217;m laying down the hammer a bit more harshly because somewhere hidden in this messy pastiche to 80s fantasy movies is an adventure worthy of the gods&#46; Unfortunately&#44; hilarity and humanity fail to harmonize in Thor&#58; Love and Thunder&#46;",
          "url": "https://www.rogerstv.com/media?lid=237&rid=2&gid=621609"
      },
      {
          "iconImage": {
              "url": "https://images.fandango.com/cms/assets/7928a380-9be3-11eb-954b-43250906bea9--fresh.png"
          },
          "criticName": "David Hogan",
          "publicationName": "hoganreviews.co.uk",
          "body": "Overall a fun time but not the big blockbuster some might be expecting and certainly not recommended if you didn&#8217;t find Ragnarok very funny&#8230;",
          "url": "https://hoganreviews.co.uk/2022/11/23/thor-love-and-thunder-review/"
      }
  ],
